<div class="navbar{{ inverse|default(false) ? ' navbar-inverse' : '' }}{{ fixedTop|default(false) ? ' navbar-fixed-top' : '' }}{{ staticTop|default(false) ? ' navbar-static-top' : '' }}">
    <div class="navbar-inner">
        <div class="container{{ isFluid|default(false) ? '-fluid' : '' }}">
            {% block header %}
                {% block toggle %}
                <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </a>
                {% endblock %}
                {% block brand %}{% endblock %}
            {% endblock header %}
            <div class="">
                {% block menu_container %}
                <div class="navbar-collapse navbar-responsive-collapse">
                    {% block menu %}{% endblock %}
                </div>
                {% endblock %}
            </div>
        </div>
    </div>
</div>
